{
  "scripts": {
    "shadow-dev": "shadow-cljs -A:dev watch app",
    "shadow-release": "shadow-cljs -A:release release app",
    "postcss:watch": "cross-env TAILWIND_MODE=watch postcss src/css/tailwind.css -o ./public/css/main.css --verbose -w",
    "postcss:release": "cross-env NODE_ENV=production postcss src/css/tailwind.css -o ./public/css/main.css --verbose",
    "shadow:watch": "shadow-cljs watch app",
    "shadow:release": "shadow-cljs release app",
    "babel:watch": "babel --presets @babel/preset-typescript src/stories -d gen/juxt/card --watch --extensions .tsx,.jsx,.ts,.js",
    "babel:release": "babel --presets @babel/preset-typescript src/stories -d gen/juxt/card --extensions .tsx,.jsx,.ts,.js",
    "dev": "run-p -l *:watch",
    "release": "run-s *:release",

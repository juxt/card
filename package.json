  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{js,ts,jsx,tsx,css}": "prettier --write",
    "*.{ts,tsx}": "tsc --noEmit --jsx 'preserve'"

{
  "scripts": {
    "clean": "rm -rf gen",
    "babel:release": "babel --presets @babel/preset-typescript src/js -d gen/juxt/card --extensions .tsx,.jsx,.ts,.js",
    "postcss:release": "cross-env NODE_ENV=production postcss src/css/tailwind.css -o ./target/css/main.css --verbose",
    "shadow:release": "shadow-cljs release app",
    "shadow-dev": "shadow-cljs -A:dev watch app",
    "postcss:watch": "cross-env TAILWIND_MODE=watch postcss src/css/tailwind.css -o ./public/css/main.css --verbose -w",
    "shadow:watch": "shadow-cljs watch app",
    "babel:watch": "babel --presets @babel/preset-typescript src/js -d gen/juxt/card --watch --extensions .tsx,.jsx,.ts,.js",
    "dev": "npm run clean && run-p -l *:watch",
    "release": "run-s clean *:release",
    "debug": "shadow-cljs release app --debug",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token=f1f7695ce98b",
    "prepare": "husky install",
    "eslint:lint": "eslint src/js --fix --ext .js,.jsx,.ts,.tsx",
    "eslint:watch": "eslint src/js --watch --ext .js,.jsx,.ts,.tsx",
    "prettier:lint": "prettier --ignore-unknown --write src/js",
    "typecheck:lint": "tsc",
    "typecheck:watch": "tsc --watch",
    "lint": "run-p -l *:lint"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@fullcalendar/daygrid": "^5.9.0",
    "@fullcalendar/interaction": "^5.9.0",
    "@fullcalendar/react": "^5.9.0",
    "@fullcalendar/timegrid": "^5.9.0",
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.3",
    "@hookform/resolvers": "^2.8.0",
    "@tailwindcss/forms": "^0.3.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/yup": "^0.29.13",
    "cssnano": "^5.0.7",
    "highlight.js": "10.7.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-contexify": "^5.0.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.13.0",
    "slate": "^0.63.0",
    "slate-react": "^0.65.0",
    "typescript": "^4.3.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.7",
    "@types/react-big-calendar": "^0.33.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "10.3.1",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.9.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "8.3.6",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "shadow-cljs": "^2.14.4",
    "tailwindcss": "2.2.7"
